{% extends 'back.html.twig' %}

{% block content %}
    <h5>Créer une catégorie</h5>
    {{ form_start(form) }}
       <div class="form-group">
          {{ form_row(form.libelle, {'attr': { 'class': 'form-control' }}) }} 
       </div>

       <div class="form-group">
         {{ form_row(form.slug, {'attr': { 'class': 'form-control' }}) }}  
       </div>

       <div class="form-group">
          {{ form_row(form.description, {'attr': { 'class': 'form-control' }}) }} 
       </div>
        <hr>
        <input class="btn btn-primary" type="submit" value="Enregister" />
   
            <a class="btn btn-default" href="{{ path('admin_category_index') }}">Retour</a>
      
 {{ form_end(form) }}
{% endblock %}

